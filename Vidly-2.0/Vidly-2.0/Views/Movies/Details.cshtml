@model Vidly_2._0.Models.Movie
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
    <li>Genre : @Model.Genre.name</li>
    <li>Released Date : @Model.ReleasedDate.ToLongDateString()</li>
    <li>Date Added : @Model.DateAded.ToLongDateString()</li>
</ul>

